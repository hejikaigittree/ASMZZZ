            <param name="x">X坐标数组 </param>
            <param name="y">Y坐标数组 </param>
            <param name="numberOfPoints">坐标点个数 </param>
            <param name="k">拟合直线的斜率 </param>
            <param name="b">拟合直线的截距 </param>
