                            <!--
                            <StackPanel Margin="0,30,0,0" Background="#FFFAFAFA">
                                <StackPanel Orientation="Horizontal">
                                    <Button Content="自动生成金线区域"
                                Width="160"
                                Command="{Binding AddRefWireUserRegionCommand}"
                                HorizontalAlignment="Left"/>
                                </StackPanel>

                                <StackPanel Orientation="Horizontal"
                            Margin="0,10,0,0">
                                    <TextBlock Text="类型："/>
                                    <TextBlock Text="{Binding CurrentGroup.RefLineUserRegions.RegionType}"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal"
                            Margin="0,10,0,0">
