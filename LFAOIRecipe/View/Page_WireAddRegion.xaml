                                        <TextBlock Text="{Binding WireParameter.Rectangle2Width}"/>
                                        <TextBlock Text="pixel" />
                                    </StackPanel>
                                    <Slider Orientation="Horizontal" 
                                            TickFrequency="2"
                                            TickPlacement="BottomRight"
                                            Minimum="1"
                                            Maximum="20"
                                            Value="{Binding WireParameter.Rectangle2Width}"                                    
                                            IsSelectionRangeEnabled="True" Width="162" 
                                            AutoToolTipPlacement="BottomRight" Margin="5,0,0,0" />
                                </StackPanel>
                            </StackPanel>


                        </StackPanel>
