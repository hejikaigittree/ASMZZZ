                        </StackPanel>
                    </materialDesign:Card>
                    <StackPanel Margin="0,20,0,0" HorizontalAlignment="Center" VerticalAlignment="Center">
                        <Button Content="显示所有区域" ToolTip="显示所有的焊点金线组区域！"
                        Width="160"
                        Command="{Binding DisplayAllRegionCommand}"
                        HorizontalAlignment="Left" Margin="0,0,0,5"/>
                    </StackPanel>
                    <materialDesign:Card Margin="5,30,5,0" >
                        <StackPanel Margin="10">
                            <StackPanel Margin="0" Background="#FFFAFAFA">
                                <StackPanel Orientation="Horizontal">
                                    <Button Content="添加金线检测区域"
                                Width="160"
                                Command="{Binding AddMultiWireRegionsCommand}"
                                HorizontalAlignment="Left"/>
                                    <Button Content="删除区域"
                                Margin="20,0,0,0"
                                Command="{Binding RemoveMultiWireRegionsCommand}"/>
                                    <Button Content="修改区域"  Margin="20,0,0,0" Height="Auto" ToolTip="区域修改完，鼠标右键确认！"
                                    Command="{Binding ModifyMultiWireRegionsCommand}"/>
                                </StackPanel>
                            </StackPanel>
                            <materialDesign:Card Margin="5,10,5,5">
                                <StackPanel>
                                    <DataGrid   Margin="0,5,0,0" IsReadOnly="False" 
                                        ItemsSource="{Binding CurrentGroup.LineUserRegions}"                             
                                        SelectedItem="{Binding SelectedUserRegion}" 
                                        AlternatingRowBackground="#D1DFE7"
                                        IsSynchronizedWithCurrentItem="True" Background="White">
                                        <DataGrid.Columns>
                                            <DataGridTemplateColumn>
                                                <DataGridTemplateColumn.HeaderTemplate>
                                                    <DataTemplate>
                                                        <StackPanel Orientation="Horizontal">
                                                            <TextBlock Text="选择"  FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                            <CheckBox  IsChecked="{Binding DataContext.IsCheckAll,UpdateSourceTrigger=PropertyChanged, Mode=TwoWay,ElementName=userControl}" 
                                                                Command="{Binding DataContext.IsCheckAllCommand,  ElementName=userControl}" IsThreeState="True"/>
                                                        </StackPanel>
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.HeaderTemplate>
                                                <DataGridTemplateColumn.CellTemplate >
                                                    <DataTemplate >
                                                        <CheckBox IsChecked="{Binding IsSelected,UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" 
                                                           Command="{Binding DataContext.IsCheckCommand,ElementName=userControl}"/>
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                            <materialDesign:MaterialDataGridTextColumn Header="序号" 
                                                           Binding="{Binding Index}" 
                                                           IsReadOnly="True"/>

                                            <DataGridTemplateColumn  Header="启用">
                                                <DataGridTemplateColumn.CellTemplate >
                                                    <DataTemplate >
                                                        <CheckBox IsChecked="{Binding IsEnable,UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"         
                                      Command="{Binding DataContext.UserRegionEnableChangedCommand, ElementName=userControl}"
                                      CommandParameter="{Binding}"
                                      Style="{StaticResource MaterialDesignCheckBox}" />
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>

                                            <!--
                                            <DataGridTemplateColumn  Header="类型">
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <ComboBox 
                                                Style="{StaticResource MaterialDesignDataGridComboBox}"
                                                SelectedIndex="{Binding IsAccept, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged,Converter={StaticResource BooleanToComboBoxIndexConverter}}"
                                                    materialDesign:ComboBoxAssist.ClassicMode="true" HorizontalContentAlignment="Center">
                                                            <ComboBoxItem Content="接受区"/>
                                                            <ComboBoxItem Content="拒绝区"/>
                                                        </ComboBox>
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                            -->
                                            <materialDesign:MaterialDataGridTextColumn Header="形状" 
                                                           Binding="{Binding RegionType}" 
                                                           IsReadOnly="True"/>

                                            <DataGridTemplateColumn  Header="检测金线算法选择">
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <ComboBox 
                                                            Style="{StaticResource MaterialDesignDataGridComboBox}"
                                                            SelectedIndex="{Binding AlgoParameterIndex, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                            materialDesign:ComboBoxAssist.ClassicMode="true" HorizontalContentAlignment="Center">
                                                            <ComboBoxItem Content="Threshold"/>
                                                            <ComboBoxItem Content="High Efficiency"/>
                                                            <ComboBoxItem Content="High Performance"/>
                                                        </ComboBox>
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                        </DataGrid.Columns>
                                    </DataGrid>
                                </StackPanel>
                            </materialDesign:Card>

                            <StackPanel Orientation="Horizontal" Margin="0,30,0,0">
                                <Expander x:Name="Exp1" HorizontalAlignment="Left" HorizontalContentAlignment="Left" AutomationProperties.IsColumnHeader="True" ExpandDirection="Right" Background="White" IsExpanded="{Binding WireParameter.IsGroupBatchParasSet}" >
                                    <Expander.Header>
                                        <TextBlock Text="启用算法参数  【按方法】 批量修改 ">
                                            <TextBlock.LayoutTransform>
                                                <RotateTransform Angle="-270"/>
                                            </TextBlock.LayoutTransform>
                                        </TextBlock>
                                    </Expander.Header>
                                    <StackPanel Orientation="Horizontal">
                                        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,10,0">
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                                                <TextBlock Text="选择批量修改组" VerticalAlignment="Center" Foreground="#FF607D8B" FontWeight="Bold"></TextBlock>
                                            </StackPanel>
                                            <materialDesign:Card Height="318" Margin="0,28,0,0" HorizontalAlignment="Center">
                                                <StackPanel HorizontalAlignment="Center">
                                                    <DataGrid IsReadOnly="False" VerticalScrollBarVisibility="Auto"
                                                ItemsSource="{Binding Groups}"                          
                                                IsSynchronizedWithCurrentItem="True" Background="White" Height="319" HorizontalAlignment="Center" FontSize="12" MinColumnWidth="5">
                                                        <DataGrid.Columns>

                                                            <materialDesign:MaterialDataGridTextColumn Header="序号" 
                                                           Binding="{Binding Index}" 
                                                           IsReadOnly="True" MinWidth="0"/>

                                                            <DataGridTemplateColumn MinWidth="0">

                                                                <DataGridTemplateColumn.HeaderTemplate>
                                                                    <DataTemplate>
                                                                        <StackPanel Orientation="Horizontal">
                                                                            <TextBlock Text="全选"  FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                                            <CheckBox IsChecked="{Binding DataContext.IsCheckAllGroup,UpdateSourceTrigger=PropertyChanged, Mode=TwoWay,ElementName=userControl}" 
                                                                                  Command="{Binding DataContext.IsCheckAllGroupCommand,  ElementName=userControl}" IsThreeState="True"/>
                                                                        </StackPanel>
                                                                    </DataTemplate>
                                                                </DataGridTemplateColumn.HeaderTemplate>

                                                                <DataGridTemplateColumn.CellTemplate>
                                                                    <DataTemplate >
                                                                        <CheckBox IsChecked="{Binding IsSelected,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                                                                              Command="{Binding DataContext.IsCheckGroupCommand,ElementName=userControl}"></CheckBox>
                                                                    </DataTemplate>
                                                                </DataGridTemplateColumn.CellTemplate>

                                                            </DataGridTemplateColumn>

                                                        </DataGrid.Columns>
                                                    </DataGrid>
                                                </StackPanel>
                                            </materialDesign:Card>
                                        </StackPanel>
                                        <StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Button Content="批量修改参数" 
                                                             Command="{Binding ModifyParametersBatchCommand}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                <Button Content="默认值" Style="{DynamicResource MaterialDesignFlatButton}"
                                                             Command="{Binding SetDefaultParasCommand}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="30,0,0,0" FontSize="12"/>
                                                <Button Content="清空参数" Style="{DynamicResource MaterialDesignFlatButton}"
                                                             Command="{Binding ClearParasCommand}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="30,0,0,0" FontSize="12"/>
                                            </StackPanel>                                         
                                            <TabControl x:Name="tabContro0" HorizontalAlignment="Center" DockPanel.Dock="Top" VerticalAlignment="Bottom" BorderBrush="#FFACACAC"                               
                                                materialDesign:ColorZoneAssist.Mode="PrimaryLight"  Background="#DDFFFFFF" Margin="0,10,0,0">
                                                <TabControl.Resources>
                                                    <Style TargetType="TabItem">
                                                        <Setter Property="Template">
                                                            <Setter.Value>
                                                                <ControlTemplate TargetType="TabItem">
                                                                    <Border Name="Border" BorderThickness="1,1,1,0" 
                                                            BorderBrush="Gainsboro" 
                                                            CornerRadius="4,4,0,0" Margin="0.5,0">
                                                                        <ContentPresenter x:Name="ContentSite"
                                                            VerticalAlignment="Center"
                                                            HorizontalAlignment="Center"
                                                            ContentSource="Header"                                                                          
                                                            Margin="12,6,12,6"/>
                                                                    </Border>
                                                                    <ControlTemplate.Triggers>
                                                                        <Trigger Property="IsSelected" Value="True">
                                                                            <Setter TargetName="Border" Property="Background" Value="#FFD6DDE0" />
                                                                        </Trigger>
                                                                        <Trigger Property="IsSelected" Value="False">
                                                                            <Setter TargetName="Border" Property="Background" Value="white" />
                                                                        </Trigger>
                                                                    </ControlTemplate.Triggers>
                                                                </ControlTemplate>
                                                            </Setter.Value>
                                                        </Setter>
                                                    </Style>
                                                </TabControl.Resources>
                                                <TabItem  x:Name="TabItem_1" Header="Threshold" Height="28" VerticalAlignment="Top" >
                                                    <StackPanel Margin="20,20,20,10" >
                                                        <DockPanel Margin="0,5,0,0" HorizontalAlignment="Center" VerticalAlignment="Center">
                                                            <Label Content="分割阈值" Width="100"
                                                HorizontalContentAlignment="Right" Foreground="#DD000000"/>
                                                            <TextBox Text="{Binding ThreshGray[0]}"  Width="50"  HorizontalAlignment="Left" VerticalAlignment="Center" VerticalContentAlignment="Center"/>
                                                            <TextBox Text="{Binding ThreshGray[1]}"  Width="50"  HorizontalAlignment="Left" VerticalAlignment="Center" VerticalContentAlignment="Center"/>
                                                        </DockPanel>
                                                        <DockPanel ToolTip="闭运算大小"  Margin="0,5,0,0" HorizontalAlignment="Center">
                                                            <Label Content="闭操作尺寸" Width="100" HorizontalContentAlignment="Right" Foreground="#DD000000"/>
                                                            <TextBox Text="{Binding ClosingSize}"  Width="100"  HorizontalAlignment="Left"/>
                                                        </DockPanel>
                                                        <DockPanel ToolTip=" "  Margin="0,5,0,0" HorizontalAlignment="Center">
                                                            <Label Content="测量宽" Width="100" HorizontalContentAlignment="Right" Foreground="#DD000000"/>
                                                            <TextBox Text="{Binding  WireWidth[0]}"  Width="50"  HorizontalAlignment="Left"/>
                                                            <TextBox Text="{Binding  WireWidth[1]}"  Width="50"  HorizontalAlignment="Left"/>
                                                        </DockPanel>
                                                        <DockPanel ToolTip=" "  Margin="0,5,0,0" HorizontalAlignment="Center">
                                                            <Label Content="测量长" Width="100" HorizontalContentAlignment="Right" Foreground="#DD000000"/>
                                                            <TextBox Text="{Binding WireLength[0]}"  Width="50"  HorizontalAlignment="Left"/>
                                                            <TextBox Text="{Binding WireLength[1]}"  Width="50"  HorizontalAlignment="Left"/>
                                                        </DockPanel>
                                                        <DockPanel ToolTip="检测面积"  Margin="0,5,0,0" HorizontalAlignment="Center">
                                                            <Label Content="测量面积" Width="100" HorizontalContentAlignment="Right" Foreground="#DD000000"/>
                                                            <TextBox Text="{Binding WireArea[0]}"  Width="50"  HorizontalAlignment="Left"/>
                                                            <TextBox Text="{Binding WireArea[1]}"  Width="50"  HorizontalAlignment="Left"/>
                                                        </DockPanel>
                                                        <DockPanel ToolTip="焊点至金线法线距离阈值判断，为0则不启用"  Margin="0,5" HorizontalAlignment="Center">
                                                            <Label Content="焊点至金线距离" Width="100" HorizontalContentAlignment="Right" Foreground="#DD000000"/>
                                                            <TextBox Text="{Binding DistTh}"  Width="100"  HorizontalAlignment="Left"/>
                                                        </DockPanel>
                                                    </StackPanel>
                                                </TabItem>
                                                <TabItem  x:Name="TabItem2_2" Header="High Efficiency" Background="{x:Null}"    >
                                                    <StackPanel Margin="20,0,20,10" >
                                                        <DockPanel Margin="0,5,0,0" HorizontalAlignment="Center" VerticalAlignment="Center">
                                                            <Label Content="检测线宽" Width="118" HorizontalContentAlignment="Right" Foreground="#DD000000"/>
